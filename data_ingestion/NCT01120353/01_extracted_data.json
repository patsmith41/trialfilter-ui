  "key_inclusion_summary": [
    "Must be a five-year survivor of specific childhood cancers like leukemia, lymphoma, CNS tumor, bone tumor, Wilms tumor, neuroblastoma, or soft tissue sarcoma.",
    "Diagnosed before the age of 21.",
    "Initial diagnosis must have occurred between January 1, 1970 and December 31, 1999.",
    "Must have been English- or Spanish-speaking and living in the U.S. or Canada at the time of diagnosis."
  ],
  "key_exclusion_summary": [
    "Diagnosis of non-malignant tumors (e.g., Langerhans cell histiocytosis, meningioma).",
    "Non-English speaking.",
    "Residence outside the US or Canada at the time of diagnosis."
  ]
}
