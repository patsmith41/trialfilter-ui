  "key_inclusion_summary": [
    "Age 18 years or older.",
    "Histologically confirmed Grade IV Glioblastoma (IDH wild-type).",
    "Karnofsky performance status of at least 60% for newly diagnosed patients or 70% for recurrent patients.",
    "Patients must have either newly diagnosed GBM or be in their first or second recurrence after initial therapy.",
    "Availability of tumor tissue from surgery or biopsy is required."
  ],
  "key_exclusion_summary": [
    "Newly diagnosed patients cannot have received any prior treatment for glioma.",
    "Recurrent patients cannot have had more than 2 prior lines of chemotherapy or prior treatment with lomustine or bevacizumab.",
    "Patients with extensive leptomeningeal disease are not eligible.",
    "Significant cardiac issues, specifically a prolonged QTc interval (>450 msec for males, >470 msec for females).",
    "History of another malignancy within the previous 2 years, with some exceptions."
  ]
