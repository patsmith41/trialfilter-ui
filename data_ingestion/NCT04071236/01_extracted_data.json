    "key_inclusion_summary": [
      "Histologically confirmed metastatic castration-resistant prostate cancer with two or more skeletal metastases.",
      "Progressive disease after at least one novel hormonal therapy (e.g., abiraterone, enzalutamide) or taxane chemotherapy.",
      "ECOG performance status of 0-2.",
      "Testosterone level should be consistently lower than 50 ng/dL.",
      "Adequate organ and marrow function (hemoglobin > 9 g/dL, ANC >= 1,500/mcL, platelets >= 100,000/mcL)."
    ],
    "key_exclusion_summary": [
      "Visible visceral organ metastases.",
      "Patients who have had chemotherapy or radiotherapy within 4 weeks prior to entering the study.",
      "Patients who have had previous hemibody external radiation.",
      "Patients who have imminent/established spinal cord compression or pathological fracture in weight bearing bones.",
      "Active autoimmune conditions or patients on chronic immunosuppression."
    ]
  }