  "key_inclusion_summary": [
    "Must have a histologically confirmed metastatic or unresectable solid malignancy.",
    "Must have a tumor lesion that can be biopsied and at least one measurable disease site.",
    "Patients in specific cohorts must have a qualifying genetic mutation (e.g., BRCA1/2, certain DDR genes, or KRAS).",
    "Must have received at least one prior line of systemic therapy in the advanced/metastatic setting.",
    "ECOG performance status of 0, 1, or 2."
  ],
  "key_exclusion_summary": [
    "Currently receiving other investigational agents.",
    "Previously received ZEN003694 or another investigational BET inhibitor.",
    "Use of medications that are strong inhibitors or inducers of CYP3A4.",
    "Uncontrolled intercurrent illness, including psychiatric conditions or significant cardiac disease.",
    "Symptomatic brain metastases requiring steroids."
  ]
}
