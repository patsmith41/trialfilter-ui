  "key_inclusion_summary": [
    "Proven unresectable or locally recurrent hepatocellular cancer.",
    "Must have 3 or fewer tumors, with specific size limitations based on the number of lesions.",
    "Zubrod performance status of 0-1.",
    "Child-Turcotte-Pugh (CTP) score of A or B7.",
    "Adequate hematologic and organ function, including platelets >= 50,000 cells/mm^3 and albumin >= 2.5 g/dl."
  ],
  "key_exclusion_summary": [
    "Definitive clinical or radiologic documentation of extrahepatic tumor (metastases).",
    "Prior radiotherapy to the region of the study cancer that would result in overlap of radiation fields.",
    "Prior liver transplant.",
    "Systemic chemotherapy for the study cancer less than 2 weeks prior to registration.",
    "HIV positive with CD4 count < 200 cells/microliter."
  ]
}