  "key_inclusion_summary": [
    "Female patients with Stage I-III breast cancer who have undergone breast conserving surgery or mastectomy.",
    "Recommended to receive postoperative radiation therapy to the breast or chest wall and comprehensive regional lymph nodes.",
    "Histologically documented breast cancer.",
    "Negative metastatic workup.",
    "ECOG performance status 0, 1, or 2."
  ],
  "key_exclusion_summary": [
    "Inflammatory breast cancer or T4b/T4c disease.",
    "Clinical or radiographic evidence of distant metastatic disease.",
    "Prior history of radiation therapy overlapping with the current target volume.",
    "History of connective tissue disorder (e.g., lupus, scleroderma).",
    "Known BRCA 1 or BRCA 2 mutation."
  ]
