  "key_inclusion_summary": [
    "Age 18 years or older.",
    "Must have pathologically confirmed low to intermediate risk invasive adenocarcinoma of the prostate (Clinical stage T1-T2c, Gleason Score <= 7, PSA <= 20 ng/ml).",
    "Planning to undergo External Beam Radiotherapy (EBRT).",
    "Able to provide written informed consent."
  ],
  "key_exclusion_summary": [
    "Prostate volume > 80 cc.",
    "Planning to undergo brachytherapy or focal boost.",
    "Evidence of metastatic disease or planned pelvic lymph node radiotherapy.",
    "History of radical prostatectomy, other ablative anti-prostate cancer therapy, or previous pelvic irradiation.",
    "Known allergy or sensitivity to polyethylene glycol (PEG) or iodine."
  ]
