    "key_inclusion_summary": [
      "Patients with pathologically proven diagnosis of lateralized Oropharyngeal Cancer (T1-3 M0).",
      "Planned definitive radiotherapy or chemoradiotherapy with bilateral neck radiotherapy.",
      "ECOG performance status of 0, 1, or 2.",
      "Patient must consent to providing a formalin-fixed paraffin block of tumor tissue for correlative marker assays.",
      "Willing to complete quality of life questionnaires."
    ],
    "key_exclusion_summary": [
      "T1-T2 cancers isolated to the tonsil fossa without significant nodal involvement.",
      "Previous head and neck cancer or multiple synchronous primary head and neck cancers.",
      "Previous induction or neo-adjuvant chemotherapy.",
      "Previous radiation therapy to the head and neck or comprehensive neck dissection.",
      "Severe, active co-morbidity such as unstable angina, recent MI, or diseases precluding radiotherapy."
    ]
