    "key_inclusion_summary": [
      "Must be a candidate for standard of care concurrent chemoradiation followed by consolidation durvalumab.",
      "Pathologically proven diagnosis of locally advanced, unresectable stage III NSCLC.",
      "Have at least one target lesion amenable to intratumoral and/or intranodal injection.",
      "ECOG performance status of 0 to 1."
    ],
    "key_exclusion_summary": [
      "Medical history of primary immunodeficiency or organ transplant requiring therapeutic immunosuppression.",
      "Another concurrent or prior primary malignancy within the last 3 years.",
      "Known allergies to study drugs (JNJ-90301900, platinum-based chemotherapy, or durvalumab).",
      "History of coagulation disorders or major thromboembolic events within 3 months.",
      "Severe or unstable cardiac conditions within the last 3 months."
    ]
