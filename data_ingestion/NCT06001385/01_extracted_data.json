    "key_inclusion_summary": [
      "Age 18 years or older.",
      "Diagnosed with a hematologic malignancy such as Acute Myeloid Leukemia (AML), Acute Lymphoblastic Leukemia (ALL), Myelodysplastic Syndromes (MDS), or Lymphoma.",
      "Requires a Mismatched Unrelated Donor (MMUD) for peripheral blood stem cell transplantation.",
      "Karnofsky Performance Status (KPS) of at least 60% or 70%, depending on the treatment regimen.",
      "Adequate heart, kidney, lung, and liver function."
    ],
    "key_exclusion_summary": [
      "A suitable HLA-matched related or 8/8 matched unrelated donor is available.",
      "History of a prior allogeneic transplant.",
      "Received an autologous transplant within the last 3 months.",
      "Has an uncontrolled bacterial, viral, or fungal infection.",
      "Patient is pregnant or breast-feeding.",
      "Presence of donor-specific HLA antibodies (DSA) with a median fluorescence intensity (MFI) greater than 3000."
    ]
  }